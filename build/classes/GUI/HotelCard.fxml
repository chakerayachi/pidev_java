<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="hotelCard" fx:id="box" prefHeight="398.0" prefWidth="299.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.HotelCardController">
   <children>
      <Label fx:id="libelle" prefHeight="28.0" prefWidth="264.0" text="Label" textAlignment="CENTER" textFill="#f4b707">
         <font>
            <Font name="Comic Sans MS Bold" size="19.0" />
         </font>
         <VBox.margin>
            <Insets left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <VBox fx:id="card" prefHeight="200.0" prefWidth="100.0">
         <children>
            <ImageView fx:id="image" fitHeight="120.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="60.0" right="10.0" top="30.0" />
               </VBox.margin>
               <image>
                  <Image url="@hhh.png" />
               </image>
            </ImageView>
            <Label fx:id="nbEtoile" prefHeight="46.0" prefWidth="182.0" text="Etoiles : " textFill="#f4b707">
               <VBox.margin>
                  <Insets left="50.0" top="40.0" />
               </VBox.margin>
               <font>
                  <Font name="Comic Sans MS Bold" size="19.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</VBox>
